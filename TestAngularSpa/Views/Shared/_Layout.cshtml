<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - TestAngularSpa</title>
	<base href="~/" />

	<link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

	<environment names="Development">
		@*<link rel="stylesheet" href="~/css/pink-bluegrey.css" />*@
		<link rel="stylesheet" href="~/css/indigo-pink.css" />
	</environment>
	<environment names="Staging, Production">
		<link rel="stylesheet" href="~/dist/indigo-pink.min.css" />
		@*<link rel="stylesheet" href="~/dist/pink-bluegrey.min.css" />*@
	</environment>
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
